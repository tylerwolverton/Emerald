Number testNum; 
Vec2 testVec2;		
Bool testBool;
String testStr;
Entity testEntity;

State Default
{
    OnEnter()
    {
        TestVec2Members();
    }
}

Function TestVec2Members()
{
    testVec2 = Vec2(x:1,y:1);

    Number newX = testVec2.x + 10;
    if(newX != 11)
    {
        PrintToConsole( text: "Vec2: Accessing x failed" color: "red" );
        return; 
    }

    testVec2.x = newX;
    if(testVec2.x != 11)
    {
        PrintToConsole( text: "Vec2: Setting x failed" color: "red" )
        return; 
    }
}