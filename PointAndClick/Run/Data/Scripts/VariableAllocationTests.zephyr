Function TestTempObjects()
{
    Vec2 compVec = Vec2(x:1, y:1);
    
    if(compVec != Vec2(x:1, y:1))
    {
        PrintToConsole( text: "Temp object not equal to local variable", color: "red" );
    }

    PrintToConsole( text: "TestTempObjects: Success", color: "green" );
}